# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-key

# Primary admin email - all lessons will be routed to this admin
# Use different values for local development vs production
PRIMARY_ADMIN_EMAIL=your-admin-email@example.com

# Zoom OAuth (optional - for automatic Zoom meeting creation)
ZOOM_CLIENT_ID=your-zoom-client-id
ZOOM_CLIENT_SECRET=your-zoom-client-secret

# Google Calendar OAuth (optional - for calendar integration)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
